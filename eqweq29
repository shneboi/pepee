-- Tab Button (SEEDS)
TabButton.Name = "TabButton"
TabButton.Parent = main
TabButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
TabButton.BorderColor3 = Color3.fromRGB(0, 85, 0)
TabButton.BorderSizePixel = 3
TabButton.Position = UDim2.new(0.02, 0, 0.048, 0)
TabButton.Size = UDim2.new(0, 90, 0, 30)
TabButton.Font = Enum.Font.GothamBold
TabButton.Text = "SEEDS"
TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TabButton.TextSize = 20.000
TabButton.TextStrokeColor3 = Color3.fromRGB(0, 170, 0)

-- Tab Button Click Functionality (Switch between Pet Spawner and Seed Spawner)
TabButton.MouseButton1Click:Connect(function()
    if isPetSpawnerActive then
        -- Hide Pet Spawner UI elements
        Age.Visible = false
        Weight.Visible = false
        Mutations.Visible = false
        PetName.Visible = false
        Spawn.Visible = false

        -- Show Seed Spawner UI elements
        SeedName.Visible = true
        SeedSpawnButton.Visible = true

        -- Update tab to Seed Spawner
        TabButton.Text = "PET SPAWNER"
    else
        -- Show Pet Spawner UI elements
        Age.Visible = true
        Weight.Visible = true
        Mutations.Visible = true
        PetName.Visible = true
        Spawn.Visible = true

        -- Hide Seed Spawner UI elements
        SeedName.Visible = false
        SeedSpawnButton.Visible = false

        -- Update tab to Pet Spawner
        TabButton.Text = "SEEDS"
    end

    -- Toggle active state
    isPetSpawnerActive = not isPetSpawnerActive
end)

-- Seed Spawn Button Functionality
SeedSpawnButton.MouseButton1Click:Connect(function()
    local seedName = SeedName.Text

    if not seedName or seedName == "" then
        warn("Seed name is required.")
        return
    end

    -- Replace with logic to spawn seeds
    print("Seed spawned:", seedName)
end)

-- Pet Spawn Button Functionality (Example: You can add logic to spawn a pet)
Spawn.MouseButton1Click:Connect(function()
    -- Add logic to spawn a pet (e.g., based on Age, Weight, Mutations, etc.)
    print("Pet spawned with the following data:")
    print("Age:", Age.Text)
    print("Weight:", Weight.Text)
    print("Mutations:", Mutations.Text)
    print("Pet Name:", PetName.Text)
end)

-- Exit Button Functionality
Exit.MouseButton1Click:Connect(function()
    ScreenGui:Destroy() -- Close the GUI
end)
