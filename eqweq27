SeedSpawnButton.TextStrokeTransparency = 0.000
SeedSpawnButton.TextWrapped = true

local isSeedTabActive = false

-- Toggle Seed Tab
SeedTabButton.MouseButton1Click:Connect(function()
    if isSeedTabActive then
        SeedSpawnerTab.Visible = false
        isSeedTabActive = false
    else
        SeedSpawnerTab.Visible = true
        isSeedTabActive = true
    end
end)

-- Spawn Seed functionality
SeedSpawnButton.MouseButton1Click:Connect(function()
    local seedName = SeedName.Text

    if not seedName or seedName == "" then
        warn("Seed name is required.")
        return
    end

    print("Seed spawned:", seedName)
end)

-- Minimize and Exit logic
Minimize.MouseButton1Click:Connect(function()
    main.Visible = not main.Visible
end)

Exit.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

