-- Function to switch tabs
local function switchTab(tab)
    if tab == "Pet Spawner" then
        petSpawnerSection.Visible = true
        seedSpawnerSection.Visible = false
    elseif tab == "Seed Spawner" then
        petSpawnerSection.Visible = false
        seedSpawnerSection.Visible = true
    end
end

-- Switch to Pet Spawner tab when clicked
petTabButton.MouseButton1Click:Connect(function()
    switchTab("Pet Spawner")
end)

-- Switch to Seed Spawner tab when clicked
seedTabButton.MouseButton1Click:Connect(function()
    switchTab("Seed Spawner")
end)

-- Your original content here for Pet Spawner
-- (e.g., the Age, Weight, Spawn Button, etc.)

-- Minimize Logic
local minimized = false
local originalSize = main.Size

Minimize.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        main.Size = UDim2.new(0, 415, 0, 60)
        for _, child in ipairs(main:GetChildren()) do
            if child:IsA("GuiObject") and not table.find({ImageLabel, Minimize, Exit, TextLabel}, child) then
                child.Visible = false
            else
                child.Visible = true
            end
        end
    else
        main.Size = originalSize
        for _, child in ipairs(main:GetChildren()) do
            child.Visible = true
        end
    end
end)

-- Close GUI when Exit button is clicked
Exit.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
